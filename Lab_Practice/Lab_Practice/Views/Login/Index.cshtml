@model Lab_Practice.Models.Admin
@{
    ViewBag.Title = "Index";
}

<h2>Log In</h2>
<form method="post" action="/Login/Index?returnUrl=@Request.QueryString["ReturnUrl"]">
    <h5>@ViewData["error"]</h5>
    <input type="text" class="form-control" name="UserName" value="@Model.UserName" placeholder="User Name.." />
    @Html.ValidationMessage("UserName", htmlAttributes: new { @class = "text-danger" })
    <input type="password" class="form-control" name="Password" value="@Model.Password" placeholder="password.." />
    @Html.ValidationMessage("Password", htmlAttributes: new { @class = "text-danger" })
    <input type="submit" class="form-control" value="submit" />
</form>
